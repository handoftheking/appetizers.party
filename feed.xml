  <author>
    <name>simplyrecipes</name>
    <uri>http://appetizers.party</uri>
    
  </author>
  <content type="html">
    &lt;h3 id=&quot;ingredients&quot;&gt;Ingredients&lt;/h3&gt;

&lt;p&gt;&lt;span&gt;&lt;input type=&quot;checkbox&quot; /&gt; 2 pounds chicken wings&lt;/span&gt;&lt;br /&gt;
